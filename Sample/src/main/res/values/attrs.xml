<resources>

    <declare-styleable name="CircleLoadingViewAttr">
        <attr name="circleRadius" format="dimension" />
        <attr name="circleSpacing" format="dimension" />
        <attr name="cycle" format="integer" />
    </declare-styleable>

    <declare-styleable name="RefreshableLayout">
        <attr name="isHeightMatchParent" format="boolean" />
        <attr name="isWidthMatchParent" format="boolean" />
        <attr name="autoRefresh" format="boolean" />
        <attr name="autoLoadMore" format="boolean" />
    </declare-styleable>

</resources>
